									"// Parse response JSON",
									"const response = pm.response.json();",
									"",
									"// Check if login was successful and token exists",
									"if (response.status === true && response.data && response.data.token) {",
									"    // Save token to environment variable named 'token'",
									"    pm.environment.set(\"token\", response.data.token);",
									"    console.log(\"Token saved to environment:\", response.data.token);",
									"} else {",
									"    console.log(\"Login failed or token not found\");",
									"}"
